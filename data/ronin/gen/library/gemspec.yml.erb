name: <%= @name %>
summary: A Ronin library.
description: <%= @name %> is a library for Ronin.

license: GPL-3
authors: <%= @author %>
email: <%= @email %>
homepage: <%= @homepage %>
has_yard: true

dependencies:
  ronin: ~> 1.0
<%- unless options[:generators].empty? -%>
  ronin-gen: ~> 1.0
<%- end -%>

development_dependencies:
  bundler: ~> 1.0.10
  yard: ~> 0.6.4
